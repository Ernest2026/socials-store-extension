{
  "manifest_version": 2,
  "name": "Social Store",
  "description": "Get your social profiles from every platform in one place",
  "version": "1.0.0",
  "author": "Chukwuemeka Victor",
  "icons": { "16": "social.png",
    "48": "social.png",
   "128": "social.png" },
  "browser_action":{"default_icon":"social.png"},
  "permissions": ["tabs"],
  "background":{
    "scripts":["background.js"],
    "persistent":false
  }
}

